    "lint": "eslint . --ext .ts,.tsx",
    "pack:d": "tfx extension create --manifests vss-extension-dev.json --output-path ./dist/",
    "pack:p": "tfx extension create --manifests vss-extension-prod.json --output-path ./dist/"
