{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "linterOptions": {
      "exclude": [
        "node_modules/**/*.*",
        "dist/**/*.*"
      ]
    },
    "jsRules": {},
    "rules": {
      "indent": [true, "spaces", 2],
      "quotemark": [true, "single", "jsx-double", "avoid-escape", "avoid-template"]
    },
    "rulesDirectory": []
}